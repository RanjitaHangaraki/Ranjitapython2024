num1=int(input("enter the first number:"))
num2=int(input("enter the second number:"))
num3=int(input("enter the third number:"))
num4=int(input("enter the fourth number:"))
if num1<=num2 and num1<=num3 and num1<=4:
  print("smallest number is:",num1)
elif num2<=num1 and num2<=num3 and num2<=4:
  print("smallest number is:",num2)
elif num3<=num1 and num3<=num2 and num3<=4:
  print("smallest number is:",num3)
else:
  print("smallest number is:",num4)
